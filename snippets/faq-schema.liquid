
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {%- for block in section.blocks -%}
    {
      "@type": "Question",
      "name": "{{ block.settings.heading }}",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "{{ block.settings.row_content }}"
      }
    }{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
}
</script>
